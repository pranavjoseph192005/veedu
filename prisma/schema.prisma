generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  uid       String  @unique
  email     String   @unique
  firstName      String?
  lastName       String?
  createdAt DateTime @default(now())
}
